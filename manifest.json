{
    "manifest_version": 2,
    "name": "Spice Up Canvas",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://canvas.northwestern.edu/"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_title": "Spice Up Canvas"
    },
    "permissions": ["tabs"],
    "web_accessible_resources": ["images/*"] 
}